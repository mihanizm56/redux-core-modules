{
  "name": "@wildberries/redux-core-modules",
  "version": "0.0.1-beta.5",
  "description": "core redux modules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:watch": "jest --watch",
    "test": "set CI=true && jest",
    "test:coverage": "jest --coverage",
    "alias-resolve": "tscpaths -p tsconfig.json -s ./lib -o ./dist",
    "compile": "tsc --declaration --emitDeclarationOnly --project tsconfig.json",
    "lint": "npx eslint . --ext .ts --fix",
    "commit": "git add . && git commit --no-edit",
    "packages-vulnerability-check": "check-audit",
    "pub": "npm publish --access=public",
    "beta": "npm publish --tag beta --access=public",
    "size": "pkgfiles",
    "build": "cross-env NODE_ENV=production webpack --mode production && npm run compile && npm run alias-resolve",
    "dev": "cross-env NODE_ENV=development webpack --mode development"
  },
  "license": "MIT",
  "dependencies": {
    "@wildberries/notifications": "0.0.1-beta.25",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@types/redux-batched-actions": "0.1.5",
