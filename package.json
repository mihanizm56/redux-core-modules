    "webpack-dev-server": "3.10.3"
  },
  "files": [
    "dist/"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
